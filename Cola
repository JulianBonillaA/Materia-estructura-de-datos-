from collections import deque

class Cola:
    def _init_(self):
        self.cola = deque()
    
    def encolar(self, elemento):
        """Agrega un elemento al final de la cola"""
        self.cola.append(elemento)
        print(f"Encolado: {elemento}")
    
    def desencolar(self):
        """Elimina y devuelve el elemento al frente de la cola"""
        if not self.esta_vacia():
            elemento = self.cola.popleft()
            print(f"Desencolado: {elemento}")
            return elemento
        else:
            print("La cola está vacía")
            return None
    
    def esta_vacia(self):
        """Verifica si la cola está vacía"""
        return len(self.cola) == 0
    
    def ver_primero(self):
        """Devuelve el elemento al frente sin eliminarlo"""
        if not self.esta_vacia():
            return self.cola[0]
        else:
            print("La cola está vacía")
            return None
    
    def tamano(self):
        """Devuelve el número de elementos en la cola"""
        return len(self.cola)

if _name_ == "_main_":
    cola = Cola()
    cola.encolar(1)
    cola.encolar(2)
    cola.encolar(3)
    print(f"El primer elemento es: {cola.ver_primero()}")
    cola.desencolar()
    cola.desencolar()
    print(f"Tamaño de la cola: {cola.tamano()}")
    cola.desencolar()
    cola.desencolar()
